{
    "openapi": "3.0.0",
    "info": {
        "title": "YektaShop",
        "description": "YektaShop API Docs",
        "contact": {
            "email": "farshidelf@yahoo.com"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/api/auth/register": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "register",
                "description": "register user",
                "operationId": "register",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "schema": "Request",
                                "title": "register",
                                "required": [
                                    "name",
                                    "email",
                                    "password",
                                    "password_confirmation"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Farshid"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "farshid@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "12345678"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Response Message"
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "Login",
                "description": "Login",
                "operationId": "login",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "schema": "Request",
                                "title": "login",
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "farshid@gmail.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "12345678"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Response Message"
                    }
                }
            }
        },
        "/api/auth/logout": {
            "get": {
                "tags": [
                    "auth"
                ],
                "summary": "Logout",
                "description": "Logout",
                "operationId": "logout",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "security": [
                    {
                        "api_token": []
                    }
                ]
            }
        },
        "/api/cart/add": {
            "post": {
                "tags": [
                    "cart"
                ],
                "summary": "add to cart",
                "description": "add to cart",
                "operationId": "add",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "schema": "Request",
                                "title": "add",
                                "required": [
                                    "product_id"
                                ],
                                "properties": {
                                    "product_id": {
                                        "type": "number",
                                        "example": "1"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "security": [
                    {
                        "api_token": []
                    }
                ]
            }
        },
        "/api/cart/remove/{item_id}": {
            "delete": {
                "tags": [
                    "cart"
                ],
                "summary": "remove from cart",
                "description": "remove from cart",
                "operationId": "remove",
                "parameters": [
                    {
                        "name": "item_id",
                        "in": "path",
                        "description": "The item id to be deleted",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "int"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "security": [
                    {
                        "api_token": []
                    }
                ]
            }
        },
        "/api/cart": {
            "get": {
                "tags": [
                    "cart"
                ],
                "summary": "cart.index",
                "description": "cart.index",
                "operationId": "cart.index",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "security": [
                    {
                        "api_token": []
                    }
                ]
            }
        },
        "/api/products": {
            "get": {
                "tags": [
                    "products"
                ],
                "summary": "products.index",
                "description": "products.index",
                "operationId": "products.index",
                "responses": {
                    "200": {
                        "description": "Response Message"
                    }
                }
            },
            "post": {
                "tags": [
                    "products"
                ],
                "summary": "products.store",
                "description": "products.store",
                "operationId": "products.store",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "schema": "Request",
                                "title": "products.store",
                                "required": [
                                    "name",
                                    "price",
                                    "quantity"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Book"
                                    },
                                    "price": {
                                        "type": "number",
                                        "example": "30000"
                                    },
                                    "quantity": {
                                        "type": "number",
                                        "example": "3"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Response Message"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "api_token": {
                "type": "apiKey",
                "description": "Bearer {token}",
                "name": "Authorization",
                "in": "header"
            }
        }
    }
}